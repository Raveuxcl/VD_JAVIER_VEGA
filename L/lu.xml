                                    <denominator>
                                        <gco:Integer>5000</gco:Integer>
                                    </denominator>
                                </MD_RepresentativeFraction>
                            </scaleDenominator>
                        </LI_Source>
                    </source>
                </LI_Lineage>
            </lineage>
        </DQ_DataQuality>
